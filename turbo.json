{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "dist/**"
      ],
      "cache": true
    },
    "start": {
      "dependsOn": [
        "^start"
      ],
      "cache": false
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "outputs": [
        ".next/**"
      ],
      "cache": false
    }
  },
  "globalDependencies": [
    "**/.env"
  ]
}